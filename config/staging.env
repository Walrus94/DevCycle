# Staging Environment Configuration
# This file contains staging-specific settings

# Environment
ENVIRONMENT=staging
DEBUG=false

# Database
DB_HOST=your-staging-db-host
DB_PORT=5432
DB_USERNAME=your-staging-db-user
DB_PASSWORD=your-staging-db-password
DB_DATABASE=devcycle_staging
DB_ECHO=false

# Security
SECURITY_SECRET_KEY=your-staging-secret-key-here
SECURITY_ALGORITHM=HS256
SECURITY_ACCESS_TOKEN_EXPIRE_MINUTES=20
SECURITY_JWT_LIFETIME_SECONDS=1200

# API
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=false
API_CORS_ORIGINS=["https://staging.yourdomain.com", "https://staging-app.yourdomain.com"]
API_CORS_CREDENTIALS=true

# SSL/TLS Configuration (REQUIRED for staging)
API_SSL_ENABLED=true
API_SSL_KEYFILE=/etc/ssl/private/staging.yourdomain.com.key
API_SSL_CERTFILE=/etc/ssl/certs/staging.yourdomain.com.crt
API_SSL_CA_CERTS=/etc/ssl/certs/ca-bundle.crt
API_SSL_CIPHERS=ECDHE+AESGCM:ECDHE+CHACHA20:DHE+AESGCM:DHE+CHACHA20:!aNULL:!MD5:!DSS
API_SSL_PROTOCOLS=["TLSv1.2", "TLSv1.3"]

# Logging
LOG_LEVEL=INFO
LOG_JSON_OUTPUT=true
LOG_LOG_FILE=/var/log/devcycle/staging.log

# Redis
REDIS_HOST=your-staging-redis-host
REDIS_PORT=6379
REDIS_PASSWORD=your-staging-redis-password
REDIS_DB=0

# Hugging Face
HF_TOKEN=your-staging-huggingface-token
HF_ORG_NAME=your-org-name

# Agent Configuration
AGENT_MAX_EXECUTION_TIME=450
AGENT_MAX_MEMORY_USAGE=1536
AGENT_ENABLE_LOGGING=true
AGENT_ENABLE_METRICS=true
AGENT_MODEL_NAME=gpt-4
AGENT_TEMPERATURE=0.7
AGENT_MAX_TOKENS=1500
AGENT_MAX_RETRIES=3
AGENT_RETRY_DELAY=1.5
