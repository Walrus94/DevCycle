# Development Environment Configuration
# This file contains development-specific settings

# Environment
ENVIRONMENT=development
DEBUG=true

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=devcycle123
DB_DATABASE=devcycle
DB_ECHO=false

# Security (development - will auto-generate if not set)
# SECURITY_SECRET_KEY=your-development-secret-key-here
SECURITY_ALGORITHM=HS256
SECURITY_ACCESS_TOKEN_EXPIRE_MINUTES=30
SECURITY_JWT_LIFETIME_SECONDS=3600

# API
API_HOST=127.0.0.1
API_PORT=8000
API_RELOAD=true
API_CORS_ORIGINS=["*"]
API_CORS_CREDENTIALS=true

# GCP Load Balancer Configuration (disabled in development)
API_GCP_LOAD_BALANCER_ENABLED=false
API_TRUST_PROXY=false

# Logging
LOG_LEVEL=DEBUG
LOG_JSON_OUTPUT=false
LOG_LOG_FILE=logs/devcycle.log

# Redis (optional)
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_DB=0

# Hugging Face (optional)
HF_TOKEN=
HF_ORG_NAME=

# Agent Configuration
AGENT_MAX_EXECUTION_TIME=300
AGENT_MAX_MEMORY_USAGE=1024
AGENT_ENABLE_LOGGING=true
AGENT_ENABLE_METRICS=true
AGENT_MODEL_NAME=gpt-3.5-turbo
AGENT_TEMPERATURE=0.7
AGENT_MAX_TOKENS=1000
AGENT_MAX_RETRIES=3
AGENT_RETRY_DELAY=1.0

# Docker Services
DOCKER_POSTGRES_DB=devcycle
DOCKER_POSTGRES_USER=postgres
DOCKER_POSTGRES_PASSWORD=devcycle123
DOCKER_POSTGRES_PORT=5432
DOCKER_PGADMIN_EMAIL=admin@devcycle.dev
DOCKER_PGADMIN_PASSWORD=admin123
DOCKER_DEFAULT_ADMIN_USERNAME=admin
DOCKER_DEFAULT_ADMIN_PASSWORD=admin123
DOCKER_DEFAULT_USER_USERNAME=user
DOCKER_DEFAULT_USER_PASSWORD=user123
