[run]
source = devcycle
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */env/*
    */.venv/*
    */migrations/*
    */alembic/*
    */examples/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    # Type checking
    if TYPE_CHECKING:
    from typing import TYPE_CHECKING

show_missing = True
skip_covered = False
precision = 2
fail_under = 30

[html]
directory = htmlcov
title = DevCycle Coverage Report

[xml]
output = coverage.xml
